{
  "name": "@annoto/playkit-plugin",
  "version": "0.1.1",
  "description": "Annoto Plugin for Kaltura Playkit (V7)",
  "license": "Apache-2.0",
  "author": "Genadi Sokolov [genadi@annoto.net]",
  "main": "dist/plugin.js",
  "engines": {
    "node": ">=16.0"
  },
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "npm run clean",
    "build": "webpack --env envName=prod --progress",
    "dev": "webpack serve --env envName=dev",
    "dev:proxy": "webpack serve --env envName=dev_proxy",
    "watch": "webpack --progress --color --watch --env envName=dev",
    "eslint": "eslint . --color",
    "prettier:fix": "prettier --write ."
  },
  "devDependencies": {
    "@annoto/widget-api": "^3.28.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "autoprefixer": "^10.4.7",
    "css-loader": "^6.7.1",
    "eslint": "^8.20.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "kaltura-player-js": "https://github.com/kaltura/kaltura-player-js.git#master",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0"
  },
  "peerDependencies": {
    "kaltura-player-js": "https://github.com/kaltura/kaltura-player-js.git#master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Annoto/playkit-plugin.git"
  },
  "keywords": [
    "annoto",
    "kaltura",
    "player",
    "playkit-js",
    "plugin",
    "v7"
  ]
}
